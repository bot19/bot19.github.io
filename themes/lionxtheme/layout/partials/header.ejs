<!--
	Header partial
	* contains logo + timer
-->

<header class="section is-p0 header t-fixed">
	<div class="container">
		<div class="columns">

			<div class="column">
				<div class="is-flex header__container">
					
					<!-- logo -->
					<div class="header__logo">
						<a class="logo has-text-white" href="#">
							LXL
						</a>
					</div>

					<!-- timer -->
					<div class="header__timer has-text-white">
						<i class="fas fa-circle-notch fa-spin"></i>
						<span id="hour">0</span>:<span id="mins">00</span>
						day <span id="day">0</span>,
						year <span id="year">000</span>
					</div>

				</div>
			</div>
		</div>
	</div>
</header>







