<!--
	Header partial
	* contains logo + timer
-->

<div class="g-progress t-fixed" id="fn-progress"></div>

<header class="section is-p0 header t-fixed" id="fn-header">
	<div class="container">
		<div class="columns">

			<div class="column">
				<div class="is-flex header__container">
					
					<!-- logo -->
					<div class="header__logo">
						<a href="#" id="fn-logo">
							<i class="fas fa-hourglass-half t-relative"></i><span>LXL</span>
						</a>
					</div>

					<!-- timer -->
					<div class="header__timer" id="fn-timer">
						<i class="fas fa-circle-notch fa-spin"></i>
						<span id="hour">0</span>:<span id="mins">00</span>
						day <span id="day">0</span>,
						year <span id="year">000</span>
					</div>

				</div>
			</div>
		</div>
	</div>
</header>







